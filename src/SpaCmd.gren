module SpaCmd exposing (..)

import PageId exposing ( PageId )


type SpaCmd msg
    = BaseCmd (Cmd msg)
    | ChangePage PageId
    | CopyId String


none : SpaCmd msg
none =
    BaseCmd Cmd.none


map : (a -> msg) -> SpaCmd a -> SpaCmd msg
map f msg =
    case msg of
        ChangePage id ->
            ChangePage id

        CopyId id ->
            CopyId id

        BaseCmd cb ->
            BaseCmd (Cmd.map f cb)

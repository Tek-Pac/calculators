module PageId exposing (..)



type PageId
    = Home
    | KFactor


stringToPageId : String -> Maybe PageId
stringToPageId str =
    case str of
        "" ->
            Just Home

        "#/" ->
            Just Home

        "#/k-factor" ->
            Just KFactor

        _ ->
            Nothing


pageIdToString : PageId -> String
pageIdToString id =
    case id of
        Home ->
            "#/"

        KFactor ->
            "#/k-factor"
